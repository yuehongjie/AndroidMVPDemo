<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yu.androidmvpdemo">

    <uses-permission android:name="android.permission.INTERNET"/> <!-- 联网权限 -->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="Main" />
        <activity android:name=".databinding.DataBindingDemoActivity" />
        <activity android:name=".listmvp.view.ListMVPActivity"
            android:label="@string/text_use_mvp_list"/>
    </application>

</manifest>